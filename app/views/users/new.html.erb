<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <p>
    <%= f.label :username, :id => "name"%>
    <%= f.text_field :username%>
  </p>
  
  <p>
    <%= f.label :password%>
    <%= f.password_field :password%>
  </p>

  <p>
    <%= f.label :type%>
    <%= f.select :typed,[["fulltime"],["parttime"]]%>
  </p>
  <%= f.submit %>
<%end%>
<p>
  <%= link_to 'Back', welcome_path, method: :get %>
<p>

